CREATE TABLE city (
	id 			  INTEGER NOT NULL AUTO_INCREMENT,
	region_id	  INTEGER,
    name          VARCHAR(255) NOT NULL,
    population    INTEGER,
    latitude      DECIMAL(16, 8),
    longitude     DECIMAL(16, 8),
    CONSTRAINT fk_city_region FOREIGN KEY (region_id) REFERENCES region(id),
    PRIMARY KEY(id)
)